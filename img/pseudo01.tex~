\[\begin{tikzcd}
	&& {u^{*}} \\
	\\
	{(\id_{I})^{*} \circ u^{*}} && {u^{*}} && {u^{*} \circ (\id_{J})^{*}}
	\arrow[Rightarrow, no head, from=1-3, to=3-3]
	\arrow["{\eta_{I}u^{*}}"', curve={height=12pt}, from=1-3, to=3-1]
	\arrow[curve={height=-12pt}, from=1-3, to=3-5]
	\arrow["{\varepsilon_{\id_{I},u}}"', from=3-1, to=3-3]
	\arrow["{\varepsilon_{u,\id_{J}}}", from=3-5, to=3-3]
\end{tikzcd}\]